{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-plusplus": "off",
      "no-alert": "off",
      "no-console": "off"
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": 2018,
        "sourceType": "module",
        "requireConfigFile": false
    }
}